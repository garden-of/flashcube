{
  "expo": {
    "name": "flashcube",
    "slug": "app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/flashcube-icon.png",
    "splash": {
      "image": "./assets/images/ios-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}